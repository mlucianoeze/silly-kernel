{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "build-kernel",
            "type": "cargo",
            "command": "build",
            "args": [
                "--target",
                "aarch64-unknown-none"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "presentation": {
                "echo": true,
                "reveal": "always",
                "focus": false,
                "panel": "shared",
                "showReuseMessage": true,
                "clear": false
            },
            "problemMatcher": [
                "$rustc"
            ]
        },
    ]
}
